<?php
// Here your code !

# 普通の言葉でインプット
$text = fgets(STDIN);

# ジャパリ語辞典(ボキャ貧なので、増やしてください)
$j_dictionary = array(
    "すご" => "すっごーい!",
    "すごい"=>"すごーい!",
    "たのしい"=>"たーのしー!",
    "知ってた?"=>"知ってたー?",
    "おいしい"=>"おいしー!",
    "えらい"=>"えらーい!"
);

#ジャパリ語変換($j_dictionaryのキーに該当するものがあります　→　キーに対応した値を返します)
if (array_key_exists($text, $j_dictionary) == true) {
 print($j_dictionary[$text]);    
}

?>
